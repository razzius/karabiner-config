{
  "title": "hyper ret to autocomplete",
  "rules": [
    {
      "description": "c-s to autocomplete",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["control", "shift", "command"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            },
            {
              "key_code": "down_arrow"
            },
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    }
  ]
}
