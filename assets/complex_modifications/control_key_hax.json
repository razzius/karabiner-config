{
  "title": "control key hax",
  "rules": [
    {
      "description": "c-q to bacward word",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "left_arrow",
            "modifiers": ["left_option"]
          }]
        }
      ]
    },
    {
      "description": "c-b to backward",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "left_arrow"
          }]
        }
      ]
    },
    {
      "description": "c-9 and c-0 to ( and )",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "9",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "9",
            "modifiers": ["shift"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "0",
            "modifiers": ["shift"]
          }]
        }
      ]
    }
  ]
}
