{
  "title": "Open workflowy c-m-w",
  "rules": [
    {
      "description": "Open workflowy c-m-w",
      "manipulators": [
        {
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["control", "command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Workflowy.app"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
