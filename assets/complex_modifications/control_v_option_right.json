{
  "title": "control v option-right",
  "rules": [
    {
      "description": "c-v to forward word",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [{
            "key_code": "right_arrow",
            "modifiers": ["option"]
          }]
        }
      ]
    }
  ]
}
