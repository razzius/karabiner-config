{
  "title": "Open messages c-m-m",
  "rules": [
    {
      "description": "Open messages c-m-m",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["control", "command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Messages.app"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
