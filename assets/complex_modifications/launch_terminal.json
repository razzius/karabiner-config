{
  "title": "Open terminal c-m-t",
  "rules": [
    {
      "description": "Open terminal c-m-t",
      "manipulators": [
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": ["control", "command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Terminal"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": ["control", "option"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Terminal"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
