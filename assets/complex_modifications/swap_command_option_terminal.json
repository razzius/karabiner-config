{
  "title": "Swap cmd and option in terminal",
  "rules": [
    {
      "description": "Swap cmd and option in terminal",
      "manipulators": [
        {
          //"type": "basic",
          // "conditions": [
          //  {
          //    "bundle_identifiers": [
          //      "com.apple.Terminal"
          //    ],
          //    "type": "frontmost_application_if"
