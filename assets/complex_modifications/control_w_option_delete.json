{
  "title": "control w option-delete",
  "rules": [
    {
      "description": "c-w to delete backwards word",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "delete_or_backspace",
            "modifiers": ["left_option"]
          }]
        }
      ]
    }
  ]
}
